{
  "compilerOptions": {
    "incremental": true,
    "target":"ES2015",
    "outDir": "dist",
    "module": "ESNext",
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
  },
  "include": ["env.d.ts", "**/.vitepress/**/*"]
}
